{
  "name": "course-player-react",
  "version": "1.0.0",
  "description": "Course Player built with React and Socket.IO",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development npm run open:src",
    "dev": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node tools/server.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "echo \"Error: no test specified\"",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "npx -p @babel/cli -p @babel/core babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "compile": "npx -p @babel/cli -p @babel/core babel ./src -d ./compiled",
    "build2": "NODE_ENV=production webpack --env production --config ./webpack.config.prod.js"
  },
  "keywords": [
    "react"
  ],
  "author": {
    "name": "Rong Zhuang",
    "email": "jojozhuang@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "express": "4.18.2",
    "glob-parent": "6.0.2",
    "npm-run-all": "4.1.5",
    "react": "16.14.0",
    "react-bootstrap": "0.33.1",
    "react-dom": "16.14.0",
    "socket.io": "4.5.4",
    "socket.io-client": "4.5.4",
    "styled-components": "5.3.6"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/node": "7.20.7",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "babel-loader": "9.1.2",
    "clean-webpack-plugin": "1.0.1",
    "eslint": "8.32.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-react": "7.32.1",
    "eslint-watch": "7.0.0",
    "eventsource-polyfill": "0.9.6",
    "html-webpack-plugin": "5.5.0",
    "serve-favicon": "2.5.0",
    "webpack": "5.76.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-middleware": "5.3.3",
    "webpack-hot-middleware": "2.25.3"
  },
  "engines": {
    "node": "16.13.1",
    "npm": "8.1.2"
  }
}
